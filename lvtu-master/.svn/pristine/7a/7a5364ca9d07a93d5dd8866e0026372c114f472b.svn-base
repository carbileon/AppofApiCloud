
html, body {		
    height: 100%;		
    min-width: 320px;		
}		
body {		
    font-size: 14px;		
}		
/*header*/		
.header {		
    display: -webkit-box;		
    display: -webkit-flex;		
    display: flex;		
    border-bottom: 1px solid #E5E5E5;		
    -webkit-box-pack: center;		
    -webkit-box-align: center;		
    background-color: #E5E5E5;		
}		
.header-title {		
    -webkit-box-flex: 1;		
    -webkit-flex: 1;		
    flex: 1;		
}		
.header a {		
    display: block;		
    background-size: 20px;		
    padding: 20px;		
    background-repeat: no-repeat;		
    background-position: center;		
}		
.header .back {			
    background-image: url("../image/left.png");		
}		

.header .logo {		
    background-size: 20px;		
    background-image: url("../image/logo.png");		
}		




 #loginTip{
    text-align: center;
    margin-top: 20px;
 }
 #loginTip a{
 	line-height: 40px;
    text-decoration: underline;
    color: #bfbfbf;
    font-size: 1em;
    font-family: '微软雅黑';
 }
 .hidden{
    display: none;
 }
 .input-wrap{
 	padding: 20px 10px 0;
 }
#main h2{
   
    height: 30px; font-size: 0;
    background-color: #f2f2f2;
    background-position: 10px 60%;
    background-repeat: no-repeat;
    -webkit-background-size: 142px 15px;
    background-size: 142px 15px;
  
  
}
#main ul{
    background-color: #fff;
    -webkit-box-shadow: 0px 2px 4px #dddcdc;
    box-shadow: 0px 2px 4px #dddcdc;
    margin-bottom: 6px;
}
#main ul li{
    float: left; width: 25%; border-right: 1px solid #d9d9d9;
    border-bottom: 1px solid #d9d9d9; text-align: center; 
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
#main ul li a{
    display: block; padding: 55px 0 10px;
    background-position: center 10px;
    background-repeat: no-repeat;
    -webkit-background-size: 35px;
    background-size: 35px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
#main ul li a.active{
	background-color: #f2f2f2 !important;
}

#main h2.travel{
    background-image: url(../image/travelbackground.png);
}

.input-wrap{
    padding: 20px 10px; position: relative;
}
.txt{
    border: 1px solid #e6e6e6; border-radius: 6px; padding: 10px;
    width: 100%; outline: none;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
.light{
    border-color: #55abce;
}
.del{
    display: inline-block; background: #e6e6e6 url(../image/del.png) no-repeat center;
    width: 18px; height: 18px; position: absolute; right:20px; top: 30px;
    border-radius: 18px; 
    -webkit-background-size: 10px 10px;
    background-size: 10px 10px;
    visibility: hidden;
}
.show{
    visibility:visible;
}


        body.youji-frame{		
            background-color: #EDEDED;		
        }		
        body.youji-frame #wrap{		
            display: block;		
        }		
        #yj.youji-frame{		
            padding: 0.313em 0.625em;		
            background-color: #EDEDED;		
        }		
        #yj.youji-frame .youji-box{		
            background-color: #FFFFFF;		
            width: 100%;		
            border-radius: 2px;		
            /**/		
            margin-bottom: 0.56em;		
        }		
        #yj.youji-frame .youji-top{		
            padding-left: 0.75em;		
            padding-top: 0.75em;		
        }		
        .youji-top .youji-icon-box{		
            width: 42px;		
            height: 42px;		
        }		
        .youji-top .youji-icon-box img{		
            width: 42px;		
            border-radius: 21px;		
            display: block;		
        }		
        .youji-top .user-name{		
            height: 42px;		
            line-height: 42px;		
            text-indent: 0.75em;		
            color: #3B3D42;		
        }		
        .youji-box .youji-content-text{		
            padding: 0.75em;		
        }		
        .youji-content-text .text{		
            color: #666554;		
            display: block;		
        }		
        .youji-box .youji-content-img{		
            padding-bottom: 0.75em;		
        }		
        .youji-box .youji-content-img img{		
            width: 100%;		
            display: block;		
        }		
        .youji-box .status-box{		
            padding-left: 0.75em;		
        }		
        .status-box span{		
            font-size: 0.625em;		
            color: #969696;		
            display: inline-block;		
        }		
        .youji-box .btns-group{		
            height: 63px;		
        }		
        .btns-group .icon{		
            width: 63px;		
            height: 63px;		
        }		
        .btns-group .icon.pull-left{		
            margin-right: 0.56em;		
        }		
        .btns-group .icon.pull-right{		
        }		
        .btns-group .icon-agree{		
            background: url(../image/operation_support.png) center center no-repeat;		
            background-size: 100%;		
        }		
        .btns-group .icon-agree:active,.btns-group .icon-agree.tap-active,.btns-group .icon-agree.active{		
            background: url(../image/operation_support_press.png) center center no-repeat;		
            background-size: 100%;		
        }		
        .btns-group .icon-disagree{		
            background: url(../image/operation_unsupport.png) center center no-repeat;		
            background-size: 100%;		
        }		
        .btns-group .icon-disagree:active,.btns-group .icon-disagree.tap-active,.btns-group .icon-disagree.active{		
            background: url(../image/operation_unsupport_press.png) center center no-repeat;		
            background-size: 100%;		
        }		
        .btns-group .icon-comment{		
            background: url(../image/operation_comments.png) center center no-repeat;		
            background-size: 100%;		
        }		
        .btns-group .icon-share {		
            float: right;		
            background: url(../image/operation_more.png) center center no-repeat;		
            background-size: 100%;		
        }		
        .btns-group .icon-comment:active,.btns-group .icon-comment.tap-active,.btns-group .icon-share:active,.btns-group .icon-share.tap-active{		
            background-color: #E5E5E5;		
            background-size: 100%;		
        }		
        .youji-box .btns-group{		
            height: 63px;		
        }		
        /*common*/		
        .pull-left{		
            float: left;		
        }		
        .pull-right{		
            float: right;		
        }		
        .clearfix:after{		
            content: '';		
            display: block;		
            clear: both;		
        }	


