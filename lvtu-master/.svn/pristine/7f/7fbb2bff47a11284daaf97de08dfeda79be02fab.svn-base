
.title {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;    
    float:left;
}
.title a{
    display: block;
    padding: 20px;
    display: block;	
    background-size: 50px;	
    background-repeat: no-repeat;		
    background-position: center;
    font-size: 15px;	
    
}

.center_title {
	margin-top:20px;
	margin-right:10px;
	width:120px;
    line-height:10px;
    font-size: 15px;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    text-align:left;
    color:#333333;
    
}

.titleinfo{
	
	margin-top:20px;
    line-height:10px;
    font-size: 15px;
    -webkit-box-flex: 1;
