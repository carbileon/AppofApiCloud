header {
	background-color: #f2f2f2;
	background-image: url("../image/win_yjview/top-label.png");
	border-bottom: 1px solid #f2f2f2;
}
.header {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;    
}
.header a{
    display: block;
    padding: 20px;
    display: block;		
    background-size: 20px;	
    background-repeat: no-repeat;		
    background-position: center;	
}
.title {
    line-height:40px;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    text-align: center;
    color: #FFF;	
}

