.header .ok {

    background-size: 20px;	
	background-image: url("../image/win_yjview/ok-white.png");		
}


.guideimg {
	margin-top:10px;
	text-align:center;
}
 .dateofview{
	display: block;
    padding: 30px;
    display: block;	
    background-size: 50px;	
    background-repeat: no-repeat;		
    background-position: center;
    font-size: 15px;	
	text-align:center;
	background-image: url("../image/win_yjview/calendar.png");
}     
.dayview .photo{
	padding-top:10px;
}
.dayview .photo .photoinfo{
	float:left;
}   